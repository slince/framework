{
    "name": "slince/slince",
    "description": "The Slince Framework",
    "type": "library",
    "keywords": ["slince", "framework"],
    "license": "Apache-2.0",
    "homepage": "http://www.qimuyu.com",
    "authors": [
        {
            "name": "tao",
            "email": "taosikai@yeah.net"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/http-foundation": "~3.0",
        "symfony/filesystem": "~3.0",
        "cakephp/orm": "~3.0",
        "cakephp/cache": "~3.0",
        "monolog/monolog": "~1.0",
        "filp/whoops": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "replace": {
        "slince/application": "self.version",
        "slince/config": "self.version",
        "slince/event": "self.version",
        "slince/di": "self.version",
        "slince/cache": "self.version",
        "slince/session": "self.version",
        "slince/view": "self.version",
        "slince/routing": "self.version",
        "slince/monolog-bridge": "self.version",
        "slince/cake-bridge": "self.version",
        "slince/whoops-bridge": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Slince\\": "src"
        }
    },
    "minimum-stability": "stable"
}
